{
  "name": "vehicle-inspection-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A React application for vehicle inspections",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite --host",
    "dev:backend": "cd server && npm run dev",
    "build": "tsc && vite build",
    "build:prod": "NODE_ENV=production tsc && vite build",
    "build:vercel": "npm run build",
    "preview": "vite preview",
    "start": "cd server && npm start",
    "start:prod": "NODE_ENV=production cd server && npm start",
    "serve": "npm run build && npm run preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:websocket": "node test-websocket.js",
    "deploy:vercel": "vercel --prod",
    "deploy:render": "echo 'Backend will auto-deploy on push to main branch'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.1.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.1",
    "@types/lodash": "^4.17.18",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "axios": "^1.9.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "heic2any": "^0.0.4",
    "html2canvas": "^1.4.1",
    "html5-qrcode": "^2.3.8",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.21",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.4",
    "quagga": "^0.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
